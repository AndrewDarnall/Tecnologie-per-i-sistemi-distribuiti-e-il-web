<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>FakeF</servlet-name>
    <servlet-class>FakeFlix</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Complete</servlet-name>
    <servlet-class>Completed</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FakeF</servlet-name>
    <url-pattern>/showHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Complete</servlet-name>
    <url-pattern>/completed</url-pattern>
  </servlet-mapping>
</web-app>
<!--

  The order MATTERS for the two servlet mappings!

 -->